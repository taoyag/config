{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "vue",
    "php",
    "scala"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "vue"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "rest-client.enable": true,
  "rest-client.showHeaders": true,
  "rest-client.proxy": "",
  "rest-client.proxyStrictSSL": false,
  "rest-client.excludeHostsForProxy": [],
  // codelens
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "languageserver": {
    "vls": {
      "command": "vls",
      "filetypes": ["vue"]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    }
  },
  "vetur.format.defaultFormatter.html": "prettier",
  "cSpell.userWords": [
    "Autowired",
    "DYNAMODB",
    "Dispatchable",
    "Dreamonkey",
    "IMAGEPATH",
    "JIRA",
    "Laravel",
    "Nuxt",
    "Queueable",
    "Tetsuo",
    "Tymon",
    "UNSUBMITTED",
    "Unprocessable",
    "arraybuffer",
    "camelcase",
    "chmod",
    "cidr",
    "cond",
    "connecteds",
    "datetime",
    "dont",
    "echodoc",
    "jobsearch",
    "manuallies",
    "mkdir",
    "mysqladmin",
    "nuxtjs",
    "phpcs",
    "phpstan",
    "phpunit",
    "reviewdog",
    "roid",
    "snakecase",
    "systemvars",
    "unanalyzable",
    "unanalyzables",
    "uploader",
    "vetur",
    "vuetify",
    "vuex",
    "whielist"
  ]
}
