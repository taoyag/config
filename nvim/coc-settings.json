{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx",
    "vue",
    "php",
    "scala"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx",
    "vue"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  // codelens
  "codeLens.enable": true,
  "java.referencesCodeLens.enabled": true,
  "java.jdt.ls.vmargs": "-javaagent:/usr/local/lib/lombok/lombok.jar",
  "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
  "java.format.settings.profile": "GoogleStyle",
  "languageserver": {
    "vls": {
      "command": "vls",
      "filetypes": ["vue"]
    },
    "terraform": {
      "command": "terraform-lsp",
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["haskell", "lhaskell"]
    }
  },
  "vetur.format.defaultFormatter.html": "prettier",
  "cSpell.userWords": [
    "Autowired",
    "DBIO",
    "DYNAMODB",
    "Dispatchable",
    "Dreamonkey",
    "IMAGEPATH",
    "JIRA",
    "Jdbc",
    "Laravel",
    "Nuxt",
    "Queueable",
    "Tetsuo",
    "Tymon",
    "UNSUBMITTED",
    "Unprocessable",
    "arraybuffer",
    "camelcase",
    "chmod",
    "cidr",
    "connecteds",
    "datetime",
    "dont",
    "echodoc",
    "manuallies",
    "mkdir",
    "mysqladmin",
    "nuxtjs",
    "phpcs",
    "phpstan",
    "phpunit",
    "reviewdog",
    "roid",
    "snakecase",
    "systemvars",
    "unanalyzable",
    "unanalyzables",
    "uploader",
    "vetur",
    "vuetify",
    "vuex",
    "whielist"
  ]
}
